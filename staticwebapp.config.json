{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/static/media/*.{png,jpg,gif,svg}",
      "/static/css/*",
      "/static/js/*"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}